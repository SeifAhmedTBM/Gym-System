{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "bugsnag/bugsnag-laravel": "^2.21",
        "chehivskiy/i18n-arabic": "^1.0",
        "doctrine/dbal": "^3.0",
        "elibyy/tcpdf-laravel": "^10.0",
        "google/apiclient": "^2.17",
        "guzzlehttp/guzzle": "^7.8",
        "jubaer/zoom-laravel": "^1.3",
        "khaled.alshamaa/ar-php": "^6.2",
        "kreait/firebase-php": "^7.15",
        "laravel/framework": "^10.1.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "laravelcollective/html": "^6.4",
        "laraveldaily/laravel-charts": "^0.1.24",
        "laraveldaily/laravel-invoices": "^3.3",
        "livewire/livewire": "^3.0",
        "maatwebsite/excel": "^3.1",
        "nuovo/spreadsheet-reader": "^0.5.11",
        "realrashid/sweet-alert": "^6.0.0",
        "spatie/laravel-medialibrary": "^10.12",
        "staudenmeir/belongs-to-through": "^2.5",
        "twilio/sdk": "^6.31",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/I18N/Arabic.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
